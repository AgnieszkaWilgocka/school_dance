{% extends 'base.html.twig' %}

{% block title %}
{{ 'title.user_data_edit'|trans }}
{% endblock %}

{% block body %}
<h1>{{ 'title.user_data_edit' }}</h1>
{{ form_start(form) }}
{{ form_widget(form) }}
<div class="form-group row float-sm-right">
    <input type="submit" value="{{ 'action.edit'|trans }}" class="btn btn-primary">
</div>
<div class="form-group row float-sm-left">
    <a href="{{ url('dance_index') }}" class="btn btn-link">
        {{ 'action.back_to_list'|trans }}
    </a>
</div>

{{ form_end(form) }}
{% endblock %}